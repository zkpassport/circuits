fn test_timestamp_to_date_many_dates() {
    assert(timestamp_to_date_string(0) == "19700101");
    assert(timestamp_to_date_string(86400) == "19700102");
    assert(timestamp_to_date_string(172800) == "19700103");
    assert(timestamp_to_date_string(259200) == "19700104");
}

// Return number of leap years from 1970 to (but not including) `year`
fn count_leap_years_since_1970(year: i32) -> i32 {
    let y = year - 1;
    let leaps_up_to_y = y / 4 - y / 100 + y / 400;
    let leaps_up_to_1969 = 1969 / 4 - 1969 / 100 + 1969 / 400;
    leaps_up_to_y - leaps_up_to_1969
}

