// Computes total days from 1970-01-01 to 01-01-[1970 + n]
fn days_in_years_since_1970(n: i32) -> i32 {
    365 * n + count_leap_years_since_1970(1970 + n)
}

fn estimate_and_adjust_year(days_input: i32) -> (i32, i32) {
    let approx_year = 1970 + days_input / 365;

    let c0 = days_in_years_since_1970(approx_year - 1 - 1970);
    let c1 = days_in_years_since_1970(approx_year - 1970);
    let c2 = days_in_years_since_1970(approx_year + 1 - 1970);

    let use_c0 = (days_input >= c0) & (days_input < c1);
    let use_c2 = days_input >= c2;

    let mut best_year = approx_year;
    let mut best_start_day = c1;

    best_year = if use_c0 { approx_year - 1 } else { best_year };
    best_start_day = if use_c0 { c0 } else { best_start_day };

    best_year = if use_c2 { approx_year + 1 } else { best_year };
    best_start_day = if use_c2 { c2 } else { best_start_day };

    (best_year, best_start_day)
}

fn date_from_days(days_input: i32) -> (i32, i32, i32) {
    let (year, year_start_day) = estimate_and_adjust_year(days_input);
    let day_of_year = days_input - year_start_day;

    let is_leap = is_leap_year(year);

    let month_starts: [i32; 13] = if is_leap {
        [0, 31, 60, 91, 121, 152, 182, 213, 244, 274, 305, 335, 366]
    } else {
        [0, 31, 59, 90, 120, 151, 181, 212, 243, 273, 304, 334, 365]
    };

    let mut month: i32 = 1;
    for i in 0..12 {
        let advance = day_of_year >= month_starts[i + 1];
        month = if advance { month + 1 } else { month };
    }

    let day: i32 = day_of_year - month_starts[(month - 1) as u32] + 1;
    (year, month, day)
}

#[test]
fn test_date_from_days() {
    let input = 20286;
    let (year, month, day) = date_from_days(input);

    assert(year == 2025);
    assert(month == 7);
    assert(day == 17);
}

