{
  "name": "circuits",
  "version": "0.8.0",
  "scripts": {
    "lint": "tsc --project src/ts/tsconfig.json --noEmit && nargo fmt --check",
    "test": "TZ=UTC jest circuits.test.ts",
    "generate": "tsx src/ts/scripts/circuit-builder.ts generate && nargo fmt",
    "compile": "tsx src/ts/scripts/circuit-builder.ts generate compile && nargo fmt",
    "compile:test": "scripts/ci-compile-circuits.sh",
    "test-contracts": "cd src/solidity && ./run-tests.sh && cd ../..",
    "package-circuits": "tsx --max-old-space-size=8192 src/ts/scripts/circuit-packager && cp target/packaged/manifest.json src/ts/tests/fixtures/circuit-manifest.json",
    "generate-fixtures": "tsx --max-old-space-size=8192 src/ts/scripts/generate-fixtures.ts"
  },
  "dependencies": {
    "@aws-sdk/client-cloudfront": "^3.730.0",
    "@aws-sdk/client-s3": "^3.730.0",
    "@aztec/bb.js": "^1.0.0-nightly.20250723",
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@lapo/asn1js": "^2.0.4",
    "@noble/ciphers": "^1.0.0",
    "@noble/curves": "^1.6.0",
    "@noble/hashes": "^1.5.0",
    "@noble/secp256k1": "^2.1.0",
    "@noir-lang/noir_js": "1.0.0-beta.8",
    "@noir-lang/noir_wasm": "1.0.0-beta.8",
    "@peculiar/asn1-cms": "^2.3.15",
    "@peculiar/asn1-ecc": "^2.3.15",
    "@peculiar/asn1-rsa": "^2.3.15",
    "@peculiar/asn1-schema": "^2.3.15",
    "@peculiar/asn1-x509": "^2.3.15",
    "@peculiar/webcrypto": "^1.5.0",
    "@peculiar/x509": "^1.12.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.10.1",
    "@types/node-forge": "^1.3.11",
    "@zkpassport/poseidon2": "^0.6.1",
    "@zkpassport/utils": "^0.22.11",
    "aws-cdk": "^2.176.0",
    "aws-cdk-lib": "^2.176.0",
    "babel-jest": "^29.7.0",
    "constructs": "^10.4.2",
    "fflate": "^0.8.2",
    "i18n-iso-countries": "^7.14.0",
    "ipfs-unixfs-importer": "^15.3.2",
    "jest": "^29.7.0",
    "node-forge": "^1.3.1",
    "ts-jest": "^29.1.2",
    "tsx": "^4.19.3",
    "typescript": "^5.0.0"
  }
